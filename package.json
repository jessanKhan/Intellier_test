    "lint": "eslint .",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'",
    "format": "prettier --check './src/**/*.{ts,tsx,js,jsx}'",
    "format:fix": "prettier --write './src/**/*.{ts,tsx,js,jsx}'",
    "rename": "react-native-rename",
    "prepare": "husky install"
  },
